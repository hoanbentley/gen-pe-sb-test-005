pipeline {
   agent {
       docker {
           label 'agent1'
           image 'maven:3.8.1-adoptopenjdk-11'
       }
   }
   stages {
      stage('Back-end') {
         steps {
           sh 'mvn --version'
         }
      }
      stage('Hello') {
         steps {
            sh 'java -version'
            echo "Get working directory"
            sh 'pwd'
         }
      }
   }
}
