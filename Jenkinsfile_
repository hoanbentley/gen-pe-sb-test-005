pipeline {
   agent {
       label 'agent1'
   }

   stages {
      stage('Back-end') {
         agent {
           docker { image 'maven:3.8.1-adoptopenjdk-11' }
         }
         steps {
           sh 'mvn --version'
         }
      }
      stage('Hello') {
         steps {
                sh 'java -version'
                echo "Get working directory"
                sh 'pwd'
         }
      }
   }
}
